{
  "extends": ["airbnb-base", "prettier/@typescript-eslint", "plugin:@typescript-eslint/recommended"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["prettier", "@typescript-eslint/eslint-plugin"],
  "settings": {
    "import/parsers": {
      "@typescript-eslint/parser": [".ts", ".tsx"]
    },
    "import/resolver": {
      "typescript": {}
    }
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": "warn"
  }
}
